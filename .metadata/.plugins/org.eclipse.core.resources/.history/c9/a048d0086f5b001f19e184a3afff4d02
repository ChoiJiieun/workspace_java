package ch10_extens_interface.family;

/**
 * Class Name   : Parent
 * Author       : GJ
 * Created Date : 2024. 8. 16.
 * Version      : 1.0
 * Purpose      : 상속
 * Description  : 기본 상속 방법
 */

public class Parent {
	private String name;
	private int age;
	
	public Parent(String name, int age) {
		// super() 부모 생성자 호출
		// java는 Object를 기본으로 상속받음
		super();
		this.name = name;
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	
}
